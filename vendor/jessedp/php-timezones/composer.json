{
  "name": "jessedp/php-timezones",
  "description": "Basic Timezone utilities aimed at Laravel",
  "keywords": [
    "timezone",
    "timezones",
    "laravel"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "jessedp",
      "email": "jessedp@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2.0",
    "illuminate/support": ">=5.0"
  },
  "autoload": {
    "psr-4": {
      "jessedp\\Timezones\\": "src/Timezones"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "jessedp\\Timezones\\TimezonesServiceProvider"
      ],
      "aliases": {
        "Timezones": "jessedp\\Timezones\\Facades\\Timezones"
      }
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0|^9.0"
  },
  "autoload-dev": {
    "psr-4": {
      "jessedp\\Timezones\\Tests\\": "tests"
    }
  },
  "minimum-stability": "stable"
}